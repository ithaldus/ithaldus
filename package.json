{
  "name": "topograph",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run dev:api & bun run dev:client",
    "dev:client": "vite",
    "dev:api": "bun --watch --watch-poll src/server/dev.ts",
    "build": "vite build && bun build src/server/serve.ts --outdir dist/server --target bun",
    "start": "NODE_ENV=production bun dist/server/serve.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun src/server/db/migrate.ts",
    "db:seed": "bun src/server/db/seed.ts"
  },
  "dependencies": {
    "@panzoom/panzoom": "^4.6.1",
    "drizzle-orm": "^0.36.0",
    "hono": "^4.6.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.468.0",
    "mac-oui-lookup": "^1.1.4",
    "multicast-dns": "^7.2.5",
    "nanoid": "^5.0.9",
    "node-routeros": "^1.6.9",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.530",
    "playwright": "^1.57.0",
    "puppeteer": "^24.34.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-image-crop": "^11.0.10",
    "react-router-dom": "^6.28.0",
    "sharp": "^0.34.5",
    "ssh2": "^1.16.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/ssh2": "^1.15.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.0",
    "drizzle-kit": "^0.29.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0"
  }
}
